{
    "manifest_version": 3,
    "name": "Griffin Logging Extension",
    "version": "2.0",
    "description": "Generate audit logs",
    "update_url": "https://dev-extensions.griffin-web.com:7443/updates-audit.xml",
    "key": "ammedigflapfemmofepbeakbmlkpgela",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "webNavigation",
        "<all_urls>",
        "storage"
    ],
    "host_permissions": [
        "https://dev-loki.griffin-web.com:7443/*",
        "https://loki.griffin-web.com:7443/*",
        "http://nginx_proxy:8082/*"
    ],
    "storage": {
        "managed_schema": "managed_schema.json"
    }
}